<h1>My Settings</h1>

<ul>
  <% unless @user_settings.nil? %>
    <% @user_settings.each do |user_setting| %>
      <li>
        <%= link_to "#{user_setting.settable.name}", user_setting %>
        <%= link_to "Delete", user_setting_path(user_setting),
                  method: :delete,
                  data: { confirm: "Confirm" } %>
      </li>
    <% end %>
  <% end %>
</ul>

<%= link_to "Add Settings", new_user_setting_path, method: :get %>
